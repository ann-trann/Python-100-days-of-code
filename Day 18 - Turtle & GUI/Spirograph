########################################## My code ##########################################
import turtle
import turtle as t
import random

tim = t.Turtle()
t.colormode(255)


def random_color():
    r = random.randint(0,255)
    g = random.randint(0,255)
    b = random.randint(0,255)
    color = (r, g, b)
    return color


tim.speed("fastest")

for _ in range(200):
    tim.color(random_color())
    tim.circle(150)
    tim.right(4)

turtle.Shape("circle")

